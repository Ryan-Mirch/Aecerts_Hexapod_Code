[sub_resource type="GDScript" id="GDScript_5ofka"]
script/source = "extends Control

@onready var selectedGaitLabel = %\"Selected Gait Label\"
@onready var underlineControls = $\"Controls Label/Underline\"
@onready var underlineMenu = $\"Menu Label/Underline\"
@onready var underlineSelectedGait = $\"Selected Gait Label/Underline\"

var menu_position = 1
var selected_gait_index = 0


var gaits = [\"Tri\", \"Ripple\", \"Wave\"]

# Called when the node enters the scene tree for the first time.
func _ready():
	Pages.MainPage = self
	open()
	


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	if Input.is_action_just_pressed(\"ui_left\"):
		menu_position -= 1
		if menu_position < 0: menu_position = 2
		setVisibility()
		
	if Input.is_action_just_pressed(\"ui_right\"):
		menu_position += 1
		if menu_position > 2: menu_position = 0
		setVisibility()
		
	if Input.is_action_just_pressed(\"ui_accept\"):
		select()
		
func open():
	setVisibility()
	setGait()
	visible = true
	print(\"main page opened\")
	set_process(true)
	
	
	
func close():
	visible = false
	set_process(false)


func setVisibility():
	for underline in get_tree().get_nodes_in_group(\"Underline\"):
		underline.visible = false
		
	
	match menu_position:
		0:underlineControls.visible = true
			
		1:underlineSelectedGait.visible = true
			
		2:underlineMenu.visible = true
		
func setGait():
	selectedGaitLabel.text = gaits[selected_gait_index]


func select():
	match menu_position:
		1:
			selected_gait_index += 1
			if selected_gait_index >= gaits.size(): selected_gait_index = 0
			setGait()
			
		0:
			Pages.ControlsPage.open()
			close()
"

